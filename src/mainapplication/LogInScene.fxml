         <font>
            <Font name="Bodoni MT Bold" size="20.0" />
         </font>
      </Label>
      <Button layoutX="228.0" layoutY="347.0" mnemonicParsing="false" onAction="#LogInbuttonOnAction" text="Log In">
         <font>
            <Font name="Bodoni MT Bold" size="20.0" />
         </font>
      </Button>
      <Button layoutX="459.0" layoutY="347.0" mnemonicParsing="false" onMouseClicked="#exitBtnOnClick" prefHeight="38.0" prefWidth="82.0" text="Exit">
         <font>
            <Font name="Bodoni MT Bold" size="20.0" />
         </font>
      </Button>
      <PasswordField fx:id="passwordField" layoutX="162.0" layoutY="218.0" prefHeight="40.0" prefWidth="214.0" />
      <ComboBox fx:id="comboBoxType" layoutX="162.0" layoutY="290.0" prefHeight="34.0" prefWidth="214.0" />
   </children>
</AnchorPane>
