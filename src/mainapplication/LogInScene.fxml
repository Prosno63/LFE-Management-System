<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainapplication.LogInSceneController">
   <children>
      <Label layoutX="79.0" layoutY="37.0" text="Live Field Experience Management System">
         <font>
            <Font name="Bodoni MT Bold" size="26.0" />
         </font>
      </Label>
      <Label layoutX="162.0" layoutY="121.0" text="Enter ID :">
         <font>
            <Font name="Bodoni MT Bold" size="18.0" />
